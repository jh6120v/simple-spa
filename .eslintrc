{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-console": 0,
    "no-restricted-syntax": 0,
    "no-unexpected-multiline": 0,
    "comma-dangle": 0,
    "class-methods-use-this": 0,
    "new-cap": [
      "error",
      {
        "newIsCap": false
      }
    ],
    "no-param-reassign": 0,
    "no-use-before-define": ["error", { "functions": false, "classes": true }]
  },
  "env": {
    "browser": true,
    "es6": true
  }
}
